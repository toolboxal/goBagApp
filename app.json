{
  "expo": {
    "name": "preciousLives",
    "slug": "goBagApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.toolboxal.goBagApp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#EB957E"
      },
      "package": "com.toolboxal.goBagApp",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#EB957E"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow PreciousLives to access your camera",
          "microphonePermission": "Allow PreciousLives to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-font",
      "expo-localization",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "945fa888-4b7c-4e9e-88fc-17419c36e359"
      }
    }
  }
}
